<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.studentapp.Controller.DashboardController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox spacing="15" alignment="CENTER" AnchorPane.topAnchor="20" AnchorPane.leftAnchor="20" AnchorPane.rightAnchor="20">
        <Label fx:id="welcomeLabel" text="Welcome!" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
        <Button text="Profile" onAction="#goToProfile" minWidth="200" />
        <Button text="Register for Course" onAction="#goToRegisterCourse" minWidth="200" />
        <Button text="View Registered Courses" onAction="#goToViewCourses" minWidth="200" />
        <Button text="Logout" onAction="#handleLogout" minWidth="200" />
    </VBox>

</AnchorPane>
