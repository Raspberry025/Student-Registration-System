<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.studentapp.Controller.RegisterCourseController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox spacing="10" alignment="CENTER" AnchorPane.topAnchor="20" AnchorPane.leftAnchor="20" AnchorPane.rightAnchor="20">
        <Label text="Register Courses" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
        <ListView fx:id="courseListView" prefHeight="200" />
        <HBox spacing="10">
            <Button text="Register" onAction="#handleRegister" />
            <Button text="Back" onAction="#handleBack" />
        </HBox>
    </VBox>
</AnchorPane>
